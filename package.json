{
  "name": "angular-cli",
  "displayName": "angular-cli",
  "description": "Run angular from vscode commandline.",
  "icon": "images/angular.png",
  "version": "1.2.0",
  "publisher": "pkosta2006",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/prashantkoshta/angular-cli-vscode/issues",
    "email": "pkosta2005@outlook.com"
  },
  "homepage": "https://github.com/prashantkoshta/angular-cli-vscode/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/prashantkoshta/angular-cli-vscode.git"
  },
  "categories": [
    "Languages",
    "Other"
  ],
  "engines": {
    "vscode": "^1.5.0"
  },
  "activationEvents": [
    "onCommand:angularcliextension.ngnew",
    "onCommand:angularcliextension.serve",
    "onCommand:angularcliextension.component",
    "onCommand:angularcliextension.directive",
    "onCommand:angularcliextension.pipe",
    "onCommand:angularcliextension.service",
    "onCommand:angularcliextension.class",
    "onCommand:angularcliextension.interface",
    "onCommand:angularcliextension.enum",
    "onCommand:angularcliextension.module",
    "onCommand:angularcliextension.build",
    "onCommand:angularcliextension.test",
    "onCommand:angularcliextension.e2e",
    "onCommand:angularcliextension.menu_component",
    "onCommand:angularcliextension.menu_directive",
    "onCommand:angularcliextension.menu_pipe",
    "onCommand:angularcliextension.menu_service",
    "onCommand:angularcliextension.menu_class",
    "onCommand:angularcliextension.menu_interface",
    "onCommand:angularcliextension.menu_enum",
    "onCommand:angularcliextension.menu_module"
  ],
  "main": "./out/src/angularcliextension",
  "contributes": {
    "commands": [
      {
        "command": "angularcliextension.ngnew",
        "title": "ng new"
      },
      {
        "command": "angularcliextension.serve",
        "title": "ng serve"
      },
      {
        "command": "angularcliextension.component",
        "title": "ng g component"
      },
      {
        "command": "angularcliextension.directive",
        "title": "ng g directive"
      },
      {
        "command": "angularcliextension.pipe",
        "title": "ng g pipe"
      },
      {
        "command": "angularcliextension.service",
        "title": "ng g service"
      },
      {
        "command": "angularcliextension.class",
        "title": "ng g class"
      },
      {
        "command": "angularcliextension.interface",
        "title": "ng g interfaces"
      },
      {
        "command": "angularcliextension.enum",
        "title": "ng g enum"
      },
      {
        "command": "angularcliextension.module",
        "title": "ng g module"
      },
      {
        "command": "angularcliextension.build",
        "title": "ng build"
      },
      {
        "command": "angularcliextension.test",
        "title": "ng test"
      },
      {
        "command": "angularcliextension.e2e",
        "title": "ng e2e"
      },
      {
        "command": "angularcliextension.menu_component",
        "title": "ng g component"
      },
      {
        "command": "angularcliextension.menu_directive",
        "title": "ng g directive"
      },
      {
        "command": "angularcliextension.menu_pipe",
        "title": "ng g pipe"
      },
      {
        "command": "angularcliextension.menu_service",
        "title": "ng g service"
      },
      {
        "command": "angularcliextension.menu_class",
        "title": "ng g class"
      },
      {
        "command": "angularcliextension.menu_interface",
        "title": "ng g interfaces"
      },
      {
        "command": "angularcliextension.menu_enum",
        "title": "ng g enum"
      },
      {
        "command": "angularcliextension.menu_module",
        "title": "ng g module"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "",
          "command": "angularcliextension.menu_component",
          "group": "pkoshta2006Angular2"
        },
        {
          "when": "",
          "command": "angularcliextension.menu_directive",
          "group": "pkoshta2006Angular2"
        },
        {
          "when": "",
          "command": "angularcliextension.menu_pipe",
          "group": "pkoshta2006Angular2"
        },
        {
          "when": "",
          "command": "angularcliextension.menu_service",
          "group": "pkoshta2006Angular2"
        },
        {
          "when": "",
          "command": "angularcliextension.menu_class",
          "group": "pkoshta2006Angular2"
        },
        {
          "when": "",
          "command": "angularcliextension.menu_interface",
          "group": "pkoshta2006Angular2"
        },
        {
          "when": "",
          "command": "angularcliextension.menu_enum",
          "group": "pkoshta2006Angular2"
        },
        {
          "when": "",
          "command": "angularcliextension.menu_module",
          "group": "pkoshta2006Angular2"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "mocha": "^2.3.3",
    "run-in-terminal": "^0.0.2",
    "rxjs": "^5.0.3",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  }
}